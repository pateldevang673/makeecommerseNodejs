include common/head

.wrapper      
  section.innerpage-hero
    img(src='/images/Home-Decor.jpg', alt='')
  .mid-scn-main
    section.bagsmore-scn
      .fix-wrap
        .breadcream
          ul
            li
              a(href='/') Home
            li.active #{category.category}
        .bagsmore-list-main.fix_height_width_img_content
          - for(var x=0;x<12;x++)
            .bagsmore-item
              img(src=global.config.variable.apiPath+'/#{categoryStore[x].storeBanner}', alt='')
              .watermark-logo
                img(src='/images/img-watermark-icon.png', alt='')
                span #{categoryStore[x].avgRating}
              .info-share-book
                .bag-info-cont
                  a.bag-title(style="display: -webkit-box; -webkit-line-clamp: 1; -webkit-box-orient: vertical; overflow: hidden;",
                    href='/brand/#{categoryStore[x].URL}') #{categoryStore[x].storeName}
                  p(style="display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden;") #{categoryStore[x].storeDiscription}
                .bookmarkshare-link
                  // a(href='#')
                  //   img(src='/images/bookmark-icon.png', alt='')
                  // a(href='#')
                  //   img(src='/images/share-icon.png', alt='')
        .view-all-btn
        a.right_button.cmn-btn(id="loadmoreshop",name="Home-Decor", style="cursor: pointer;margin-left: 495px;") Load More       
include common/footer
script.
  document.body.classList.add("body-innerpage");


