script.
  $(document).ready(function() {
      setTimeout(function(){ 
        if (localStorage.getItem('storetype') && localStorage.getItem('storetype') == "online") {
            container.classList.remove("switchOff");
            container.classList += " switchOn";
            $('.online').show()
            $('.offline').hide()
        } else {
            container.classList.remove("switchOn");
            container.classList += " switchOff";
            $('.offline').show()
            $('.online').hide()
        }
      }, 500);
    var container = document.querySelector("body");
    function onOffSwitch() {
      if (container.classList.contains("switchOn")) {
        container.classList.remove("switchOn");
        container.classList += " switchOff";
        $('.offline').show()
        $('.online').hide()
        localStorage.removeItem('storetype');
      } else {
        $('.online').show()
        $('.offline').hide()
        container.classList.remove("switchOff");
        container.classList += " switchOn";
        localStorage.setItem('storetype', "online");
      }
    }
    document.querySelector("#switchContainer").addEventListener("click", onOffSwitch, false);
    var fulSwiper = new Swiper('.main-fullwid-slider', {
      effect: 'fade',
      navigation: {
      nextEl: '.main-fullwid-slider .swiper-button-next',
      prevEl: '.main-fullwid-slider .swiper-button-prev',
      },
    });
    var colSwiper = new Swiper('#collection-swiper .swiper-container', {
